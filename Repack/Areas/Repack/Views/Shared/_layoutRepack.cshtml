@{

}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <link href="@Links_Repack.Content.themes.Start1_10_0.jquery_ui_css" rel="stylesheet" type="text/css" />
    <link href="@Links_Repack.Areas.Repack.Content.Repack_all_css" rel="stylesheet" type="text/css" />

    <script src="@Links_Repack.Scripts.jquery_2_1_1_js" type="text/javascript"></script>

    <script>$.uiBackCompat = false;</script>
    <script src="@Links_Repack.Scripts.jquery_ui_1_10_0_js" type="text/javascript"></script>
    <script src="@Links_Repack.Scripts.jquery_validate_js" type="text/javascript"></script>
    <script src="@Links_Repack.Scripts.jquery_validate_unobtrusive_js" type="text/javascript"></script>
    <style type="text/css">
        .ui-autocomplete-loading { background: white url('@Links_Repack.Areas.Repack.Content.images.ajax_loader_gif') right center no-repeat; }
    </style>
</head>
<body>
    <div id="outer">
        <div id="menu">
            <ul>
                <li class="banner"><span>DCMS Connect - Repack</span></li>
                <li><a href="@Url.Content("~")" accesskey="0">DCMS Connect Home</a></li>
                <li><a href="@Url.Action(MVC_Repack.Repack.Home.ChangeUi())">Change UI</a></li>
                @if (ViewContext.HttpContext.Trace.IsEnabled)
                {
                    <li><a href="@Url.Content("~/trace.axd")" target="trace">Trace</a> </li>
                }
                @if (ViewContext.HttpContext.User.Identity.IsAuthenticated)
                {
                    <li style="font-size: medium; color: White">Welcome @ViewContext.HttpContext.User.Identity.Name</li>
                }
                <li><a href="@Url.Action(MVC_Repack.Repack.Home.Tutorial())">Tutorial</a></li>
            </ul>
        </div>
        <div id="content">
            <div id="header">
                <h1>
                    @ViewBag.Title
                </h1>
                @if (ViewBag.Subtitle != null)
                {
                    <h2>@ViewBag.Subtitle</h2>
                }
            </div>
            @RenderBody()

        </div>
        <div id="footer">
            <p>
                Copyright &copy; 2011 Eclipse Systems <a href="http://www.eclsys.com" target="_blank">http://www.eclsys.com</a>
            </p>
        </div>
    </div>
    @RenderSection("scripts", false)
</body>
</html>
@*$Id$*@ 